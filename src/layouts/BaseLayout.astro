---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const { frontmatter, title } = Astro.props;
---

<Header />
    <section class="container mx-auto px-4 sm:px-8 lg:px-0 max-w-[720px] mb-20 mt-14">
        <h1 class="center">{title || frontmatter.title}</h1>
        <slot />
    </section>
<Footer />